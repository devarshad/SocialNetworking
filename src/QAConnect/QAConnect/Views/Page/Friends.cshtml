<div class="profile-detail partialView-container" data-bind="with:koLeftPage">
    <div class="profile-pane">
        <div class="profile-pane-header">
            <p>
                Friends...
            </p>
        </div>
        <div data-bind="if:friends().length==0">
            <a class="clearfix">
                <span class="empty-notification">No friends available</span>
            </a>
        </div>

        <ul data-bind="foreach:friends" class="list-inline">
            <li>
                <img class="Avtar" data-bind="attr: { src: Avtar}" />
                <a class="Title truncate" data-bind="text:FullName,attr: { href: '/'+Name}"></a>
            </li>
        </ul>
    </div>
</div>